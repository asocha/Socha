<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.final_project.socha"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.final_project.socha.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.final_project.socha.Game"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.final_project.socha.Scores"
            android:label="@string/title_activity_scores" >
        </activity>
        <activity
            android:name="com.final_project.socha.About"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.final_project.socha.PuzzleView"
            android:label="@string/title_activity_puzzle_view" >
        </activity>
        <activity
            android:name="com.final_project.socha.KeyPad"
            android:label="@string/title_activity_key_pad" >
        </activity>
    </application>

</manifest>